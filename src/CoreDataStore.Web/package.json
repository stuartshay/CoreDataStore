{
  "name": "CoreDataStore",
  "version": "1.0.0",
  "description": "CoreDataStore Angular2 with TypeScript and Gulp",
  "private": true,
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "compile": "./node_modules/.bin/gulp compile",
    "build": "./node_modules/.bin/gulp build",
    "ghpage": "./node_modules/.bin/gulp ghpage",
    "build:dev": "./node_modules/.bin/gulp build:dev",
    "postinstall": "./node_modules/.bin/typings install",
    "start": "concurrently --kill-others \"./node_modules/.bin/gulp watch\" \"./node_modules/.bin/lite-server --host=0.0.0.0\"",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "typings": "./node_modules/.bin/typings"
  },
  "dependencies": {
    "@angular/common": "^2.0.1",
    "@angular/compiler": "^2.0.1",
    "@angular/core": "^2.0.1",
    "@angular/forms": "^2.0.1",
    "@angular/http": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "@angular/platform-browser-dynamic": "^2.0.1",
    "@angular/router": "^3.0.1",
    "@angular/upgrade": "^2.0.1",
    "angular2-google-maps": "^0.16.0",
    "angular2-infinite-scroll": "^0.2.8",
    "angular2-toaster": "^1.1.0",
    "core-js": "2.4.1",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "jquery-slimscroll": "^1.3.8",
    "jquery.browser": "^0.1.0",
    "ng2-bootstrap": "1.1.3",
    "ng2-dnd": "^2.0.1",
    "ng2-select": "^1.1.0",
    "ng2-table": "^1.3.2",
    "ng2-translate": "^4.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "screenfull": "^3.0.2",
    "simple-line-icons": "^2.4.1",
    "systemjs": "0.19.38",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "concurrently": "^2.0.0",
    "connect-history-api-fallback": "^1.2.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-coveralls": "^0.1.4",
    "gulp-cssmin": "^0.1.7",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^2.0.1",

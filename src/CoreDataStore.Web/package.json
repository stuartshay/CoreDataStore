    "gulp-sourcemaps": "^1.6.0",
    "gulp-ts-config": "^1.0.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^2.12.0",
    "gulp-uglify": "^2.0.0",
    "http-proxy-middleware": "^0.17.0",
    "lite-server": "^2.2.0",
    "merge-stream": "^1.0.0",
    "systemjs-builder": "^0.15.31",
    "ts-node": "^1.3.0",
    "tslint": "^3.5.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
